{
  "name": "react-custom-flag-select",
  "version": "2.7.2",
  "description": "A react component for custom flag (country code) select.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwardfxiao/react-custom-flag-select"
  },
  "keywords": [
    "flag",
    "select",
    "country code",
    "area code",
    "phone code",
    "telephone code"
  ],
  "author": "Edward Xiao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edwardfxiao/react-custom-flag-select/issues"
  },
  "homepage": "https://github.com/edwardfxiao/react-custom-flag-select#readme",
  "scripts": {
    "jslint": " ",
    "babel": "BABEL_ENV=lib babel ./src/js/component --out-dir ./lib/components",
    "build": "rm -rf lib && npm run babel && npm run build_prod",
    "webpack_dev": "rm -rf dist && NODE_ENV=development ./node_modules/.bin/webpack --config ./webpack/development.config.js --progress --watch",
    "webpack_prod": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/production.config.js --progress -p",
    "build_dev": "npm run webpack_dev",
    "build_prod": "npm run webpack_prod",
    "build_umd": "./node_modules/.bin/webpack --config ./webpack/umd.global.config.js",
    "build_umd_min": "./node_modules/.bin/webpack --config ./webpack/umd.global.config.js --env build",
    "server_dev": "node_modules/.bin/webpack-dev-server --config ./webpack/development.config.js",
    "server_prod": "node_modules/.bin/webpack-dev-server --config ./webpack/production.config.js",
    "clean": "rimraf dist lib",
    "compile_webpack": "npm run clean && npm run build_umd && npm run build_umd_min",
    "compile": "npm run clean && npm run compile_webpack && npm run babel"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint": ">=4.18.2",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "inline-style-prefix-all": "^2.0.2",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-inputs-validation": "^4.0.0",
    "react-markdown": "^4.0.9",
    "rimraf": "^2.6.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.0",
    "validator": "^11.1.0",
    "webpack": "^4.19.1",
    "webpack-assets-manifest": "^3.1.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4"
  }
}
